<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/meta.ejs') %>
    <title>Game Histories</title>
    <!-- My CSS Source Start -->
    <link rel="stylesheet" href="/form.css" />
    <!-- My CSS Source End -->
  </head>
  <body>
    
    <div class="signup-section" id="2">
      <div class="signup-row">
        <!-- Menu Section -->
        <div class="signup-text">
          <h1>Game Histories</h1>
          <h3> <a class="head_a"href="/dashboard">Dashboard</a> </h3>
          <h3> <a class="head_a"href="/profile">Profile</a> </h3>
          <h3> <a class="head_a"href="/gamehistories">Game Histories</a></h3>
        </div>
        <!-- Form Section -->
        <div class="my-custom-scrollbar2 table-wrapper-scroll-y">
          <table class="table table-dark table-hover">
            <tr>
              <th scope="col">No.</th>
              <th scope="col">User ID</th>
              <th scope="col">Win</th>
              <th scope="col">Lose</th>
              <th scope="col">Computer</th>
            </tr>
            <% for (let i = 0; i < data.length; i++){ %>
            <tr>
              <td><%= i+1 %></td>
              <td><%= data[i].User.username %></td>
              <td><%= data[i].win %></td>
              <td><%= data[i].lose %></td>
              <td><%= data[i].com %></td>
            </tr>
            <% } %>
            
          </table>
        </div>
      </div>
    </div>
  </body>
</html>
